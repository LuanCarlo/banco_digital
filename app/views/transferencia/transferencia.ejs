		<script>
			$(document).ready( function(){

				function tabInicial() {
					document.getElementById("tabTed").classList.add("active");
					document.getElementById("tabDoc").classList.remove("active");

					$.ajax({
						url: '/ted',
						method: "get",
						success: function(data){
							$('#divConteudo').html(data);
						}
					});
				}

				tabInicial();

				$('#tabTed').click(function(){

					document.getElementById("tabTed").classList.add("active");
					document.getElementById("tabDoc").classList.remove("active");

					$.ajax({
						url: '/ted',
						method: "get",
						success: function(data){
							$('#divConteudo').html(data);
						}
					});
				});

				$('#tabDoc').click(function(){

					document.getElementById("tabDoc").classList.add("active");
					document.getElementById("tabTed").classList.remove("active");

					$.ajax({
						url: '/doc',
						method: "get",
						success: function(data){
							$('#divConteudo').html(data);
						}
					});
				});

			});
		</script>

